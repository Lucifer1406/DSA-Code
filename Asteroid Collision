class Solution:
    def asteroidCollision(self, asteroids: List[int]) -> List[int]:
        stack=[]
        for ele in asteroids:
            while stack and ele<0<stack[-1]:
                if abs(ele)>stack[-1]:
                    stack.pop()
                elif abs(ele)==stack[-1]:
                    stack.pop()
                    break
                else:
                    break
                
        
            else:
                stack.append(ele)
        return stack

        
